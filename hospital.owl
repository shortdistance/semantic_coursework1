        <rdfs:subClassOf rdf:resource="&hospital;Doctor"/>
        <rdfs:comment rdf:datatype="&rdfs;Literal">An anaesthetist is a doctor who specializes in giving anaesthetics to patients.</rdfs:comment>
    </owl:Class>
    


