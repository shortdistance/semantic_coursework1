    <!-- http://www.semanticweb.org/leizhang/ontologies/2017/3/hospital#Administrator -->

    <owl:Class rdf:about="&hospital;Administrator">
        <rdfs:label rdf:datatype="&rdfs;Literal">Administrator</rdfs:label>
        <rdfs:subClassOf rdf:resource="&hospital;Staff"/>
        <rdfs:comment rdf:datatype="&rdfs;Literal">not medical staff, just administrator.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/leizhang/ontologies/2017/3/hospital#Anaesthetic_Operation -->

    <owl:Class rdf:about="&hospital;Anaesthetic_Operation">
        <rdfs:label rdf:datatype="&rdfs;Literal">Anaesthetic Operation</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&hospital;Operation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&hospital;operationNeedMedicalStaff"/>
                        <owl:someValuesFrom rdf:resource="&hospital;Anaesthetist"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&hospital;Operation"/>
        <owl:disjointWith rdf:resource="&hospital;Non_Anaesthetic_Operation"/>
        <rdfs:comment rdf:datatype="&xsd;string">Anaesthetic Operation</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/leizhang/ontologies/2017/3/hospital#Anaesthetist -->

